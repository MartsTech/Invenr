{
  "name": "invenr-client",
  "version": "0.1.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check-types": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev --schema ./src/lib/db/schema.prisma"
  },
  "dependencies": {
    "@googleapis/calendar": "^3.0.0",
    "@googleapis/oauth2": "^1.0.0",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.7.1",
    "@reduxjs/toolkit": "^1.9.1",
    "lodash.isequal": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "next": "13.0.6",
    "next-auth": "^4.18.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.memoize": "^4.1.7",
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-persist": "^4.3.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.0",
    "prisma": "^4.7.1",
    "typescript": "^4.9.3"
  }
}
